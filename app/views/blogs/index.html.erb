<h1>ブログ一覧</h1>
<%= notice %>
<h3><%= link_to '新規投稿' , new_blog_path %></h3>
<table>
<tr>
    <th>タイトル</th>
    <th>内容</th>
</tr>
 <% @blogs.each do |blog| %>
<tr>
<td><%= blog.title %></td>
<td><%= blog.content %></td>
<td><%= link_to '詳細' , blog_path(blog.id) %></td>
<td><%= link_to '編集' , edit_blog_path(blog.id) %></td>
<td><%= link_to '削除' , blog_path(blog.id), method: :delete , data: {confirm: '削除していいですか？'} %></td>
</tr>
 <% end %>
</table>